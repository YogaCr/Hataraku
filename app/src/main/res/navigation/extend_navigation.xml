<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/extend_navigation"
    app:startDestination="@id/lowonganFragment">

    <fragment
        android:id="@+id/lowonganFragment"
        android:name="com.hataraku.hataraku.UI.Fragment.LowonganFragment"
        android:label="fragment_lowongan"
        tools:layout="@layout/fragment_lowongan" >
        <action
            android:id="@+id/action_lowonganFragment_to_lowonganNextFragment"
            app:destination="@id/lowonganNextFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
    </fragment>
    <fragment
        android:id="@+id/lowonganNextFragment"
        android:name="com.hataraku.hataraku.UI.Fragment.LowonganNextFragment"
        android:label="fragment_lowongan_next"
        tools:layout="@layout/fragment_lowongan_next" />
    <fragment
        android:id="@+id/transaksiFragment"
        android:name="com.hataraku.hataraku.UI.Fragment.TransaksiFragment"
        android:label="fragment_transaksi"
        tools:layout="@layout/fragment_transaksi" />
</navigation>